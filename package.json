{
  "name": "@300k/ts-sdk",
  "version": "1.0.22",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prettier": "node ./node_modules/prettier/bin-prettier.js --parser=typescript --single-quote --trailing-comma=all --write --print-width=120 '**/*.ts'",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "test": "npm run tsc && ava src/test/**/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/300k-xyz/ts-sdk.git"
  },
  "ava": {
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "src/": "dist/"
      }
    }
  },
  "author": "300K",
  "license": "ISC",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@types/node": "^18.14.6",
    "ava": "^5.2.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
